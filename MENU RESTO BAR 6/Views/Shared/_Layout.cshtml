<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Pruebas</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Pruebas.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

</head>
<body>
    <header>

        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Cafe Del 6</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Usuarios" asp-action="Index">Usuarios</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Reservas" asp-action="Index">Reservas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Productos" asp-action="Index">Productos</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Carrito" asp-action="Index">Carrito</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>


    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="footer">
                    <div class="contact-info">
            <p>
                <i class="bi bi-person">Contacto
                </i>
            </p>
            <p>
                <a target = "_blank" href="https://www.google.com/search?sca_esv=790745a775e4282d&sxsrf=ADLYWIJ6GQf54hmyF3gfhhgWL3sCkYQV1g:1725318423884&q=escuelas+ort+tel%C3%A9fono&ludocid=14793636629408944902&sa=X&sqi=2&ved=2ahUKEwi1tJ7-r6WIAxUFrpUCHZ-BALEQ6BN6BAhEEAI&biw=1745&bih=828&dpr=1.1">
                <i class="bi bi-phone"></i>
                (11) 4958-9000
                </a>
            </p>
            <p> 
                <i class="bi bi-geo-alt"></i>
                <a href="https://www.google.com/maps/place/Escuela+ORT+sede+Almagro/-34.6099963,-58.4292845,18.75z/data=!4m6!3m5!1s0x95bcca680b697a73:0xb6fa8d39f7cf0dc9!8m2!3d-34.6099906!4d-58.4291423!16s%2Fg%2F1tnbm9vv?entry=ttu&g_ep=EgoyMDI0MDgyOC4wIKXMDSoASAFQAw%3D%3D" target="_blank">Yatay 240, Almagro</a>
            </p>
                
        </div>
        <p class="follow-us"> &copy; 2024 | Seguinos en las redes</p>
        <p> 
            <i class="bi bi-instagram"></i> 
               <a target ="_blank" href="https://www.instagram.com/ortargentina/?hl=es-la">OrtArgentina </a>
            
        </p>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>